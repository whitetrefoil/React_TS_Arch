    "serve": "ts-node --no-cache -P dev/tsconfig.json dev/dev-server -d",
    "test": "rimraf test_results && jest --no-cache --no-coverage",
    "coverage": "rimraf test_results && jest --no-cache"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^4.0.0-alpha.6",
