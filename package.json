    "react-dom": "^15.6.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/chalk": "^0.4.31",
    "@types/connect": "^3.4.30",
    "@types/del": "^2.2.32",
    "@types/extract-text-webpack-plugin": "^2.1.0",
    "@types/fs-extra": "^4.0.0",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/http-proxy": "^1.12.0",
